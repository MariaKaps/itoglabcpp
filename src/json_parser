#include "json_parser.h"
#include <fstream>
#include <sstream>
#include <cctype>

bool parseJSON(const std::string& jsonStr, std::vector<MediaItem>& result) {

    std::istringstream iss(jsonStr);
    std::string line;
    
    while (std::getline(iss, line)) {
        find("\"id\"") != std::string::npos) {
            MediaItem item;

            if (item.isValid()) {
                result.push_back(item);
            }
        }
    }
    return !result.empty();
}
